{
  "extends": "airbnb",
  "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      },
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "semi": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "href-no-hash": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "space-before-function-paren": 0,
    "jsx-quotes": 0,
    "no-named-as-default": 0,
    "global-require": 0,
    "react/prop-types": 0,
    "react/no-array-index-key": 0,
    "arrow-body-style": 0,
    "no-danger": 0,
    "camelcase": 0,
    "arrow-parens": 0,
    "react/no-unescaped-entities": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "import/extensions": 0,
    "no-debugger": 0,
    "new-line-per-chained-call": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        config: 'webpack.dev.config.js'
        }
    },
    'import/extensions': [
      '.js',
      '.jsx',
    ]
  }
}
